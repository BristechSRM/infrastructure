<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="serilog:minimum-level" value="Debug"/>
    <add key="serilog:write-to:ColoredConsole"/>
    <add key="BaseUrl" value="http://*:8080"/>
    <add key="CorrespondenceUrl" value="http://{comms}:8080/correspondence"/>
    <add key="LastContactUrl" value="http://{comms}:8080/last-contact/"/>
    <add key="SessionsUrl" value="http://{sessions}:8080/sessions/"/>
    <add key="ProfilesUrl" value="http://{sessions}:8080/profiles/"/>
    <add key="AuthServiceUrl" value="http://{auth}:8080"/>
  </appSettings>
</configuration>
